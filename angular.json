{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "diplomka-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "projects/diplomka-frontend",
      "sourceRoot": "projects/diplomka-frontend/src",
      "prefix": "stim",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/diplomka-frontend",
            "index": "projects/diplomka-frontend/src/index.html",
            "main": "projects/diplomka-frontend/src/main.ts",
            "polyfills": "projects/diplomka-frontend/src/polyfills.ts",
            "tsConfig": "projects/diplomka-frontend/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/diplomka-frontend/src/favicon.ico",
              "projects/diplomka-frontend/src/assets",
              "projects/diplomka-frontend/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/diplomka-frontend/src/sass/styles.sass",
              "node_modules/@fortawesome/fontawesome-free/scss/fontawesome.scss",
              "node_modules/@fortawesome/fontawesome-free/scss/solid.scss",
              "node_modules/@fortawesome/fontawesome-free/scss/regular.scss",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/intro.js/introjs.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules/bootstrap/scss",
                "projects/diplomka-frontend/src/sass"
              ]
            },
            "scripts": [
              "node_modules/intro.js/intro.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/diplomka-frontend/src/environments/environment.ts",
                  "with": "projects/diplomka-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "projects/diplomka-frontend/src/environments/environment.ts",
                  "with": "projects/diplomka-frontend/src/environments/environment.dev.ts"
                }
              ]
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "projects/diplomka-frontend/src/environments/environment.ts",
                  "with": "projects/diplomka-frontend/src/environments/environment.qa.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "diplomka-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "diplomka-frontend:build:production"
            },
            "dev": {
              "browserTarget": "diplomka-frontend:build:dev"
            },
            "qa": {
              "browserTarget": "diplomka-frontend:build:qa"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "diplomka-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/diplomka-frontend/src/test.ts",
            "polyfills": "projects/diplomka-frontend/src/polyfills.ts",
            "tsConfig": "projects/diplomka-frontend/tsconfig.spec.json",
            "karmaConfig": "projects/diplomka-frontend/karma.conf.js",
            "assets": [
              "projects/diplomka-frontend/src/favicon.ico",
              "projects/diplomka-frontend/src/assets",
              "projects/diplomka-frontend/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/diplomka-frontend/src/sass/styles.sass",
              "node_modules/@fortawesome/fontawesome-free/scss/fontawesome.scss",
              "node_modules/@fortawesome/fontawesome-free/scss/solid.scss",
              "node_modules/@fortawesome/fontawesome-free/scss/regular.scss",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/intro.js/introjs.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules/bootstrap/scss",
                "projects/diplomka-frontend/src/sass"
              ]
            },
            "scripts": [
              "node_modules/intro.js/intro.js"
            ],
            "codeCoverage": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/diplomka-frontend/tsconfig.app.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/diplomka-frontend/e2e/protractor.conf.js",
            "devServerTarget": "diplomka-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "diplomka-frontend:serve:production"
            },
            "qa": {
              "devServerTarget": "diplomka-frontend:serve:qa"
            }
          }
        }
      }
    },
    "stim-lib-modal": {
      "projectType": "library",
      "root": "projects/stim-lib-modal",
      "sourceRoot": "projects/stim-lib-modal/src",
      "prefix": "stim-lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/stim-lib-modal/tsconfig.lib.json",
            "project": "projects/stim-lib-modal/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/stim-lib-modal/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "stim-lib-fab": {
      "projectType": "library",
      "root": "projects/stim-lib-fab",
      "sourceRoot": "projects/stim-lib-fab/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/stim-lib-fab/tsconfig.lib.json",
            "project": "projects/stim-lib-fab/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/stim-lib-fab/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/stim-lib-fab/src/test.ts",
            "tsConfig": "projects/stim-lib-fab/tsconfig.spec.json",
            "karmaConfig": "projects/stim-lib-fab/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/stim-lib-fab/tsconfig.lib.json",
              "projects/stim-lib-fab/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "diplomka-frontend"
}
