    "start:qa": "ng serve -c=qa --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start:prod": "node server.js",
    "build": "ng build",
    "test": "nx test --codeCoverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e -c=qa",
    "e2e:dev": "ng e2e -c=qa --watch",
    "affected:apps": "nx affected:apps",
