    "@types/socket.io": "^2.1.8",
    "codelyzer": "~5.2.2",
    "cypress": "^4.7.0",
    "istanbul-api": "2.1.7",
    "istanbul-lib-coverage": "^3.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.1.0",
    "jest": "25.5.4",
    "jest-preset-angular": "8.2.0",
    "prettier": "2.0.5",
    "ts-jest": "25.5.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  }
}
