    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "doc": "npx @compodoc/compodoc -p apps/stimulator/tsconfig.doc.json -n \"Neuro-stimulator client documentation\" -a images",
    "check-translations": "ts-node --pretty --project=tools/tsconfig.tools.json tools/check-translations.ts",
    "check-unused-selectors": "ts-node --pretty --project=tools/tsconfig.tools.json tools/check-unused-selectors.ts",
    "merge-coverage": "tsc merge-coverage.ts && node merge-coverage",
    "release": "ng build --prod --base-href=/ --output-hashing none",
    "postinstall": "ngcc --properties es2015 browser module main && node version.js",
