{
  "name": "@stechy1/diplomka-frontend",
  "version": "1.0.0",
  "keywords": [],
  "author": "Petr Štechmüller",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start:dev": "nx serve -c=dev --host 0.0.0.0 --proxy-config proxy.config.json",
    "start:qa": "ng serve -c=qa --host 0.0.0.0 --proxy-config proxy.config.json",
    "build": "ng build",
    "test": "nx test --codeCoverage",
    "test:ci": "nx test --browsers ChromeHeadless --codeCoverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e -c=qa",
    "e2e:dev": "ng e2e -c=qa --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "check-translations": "tsc apps/stimulator/check-translations.ts && node apps/stimulator/check-translations",
    "check-unused-selectors": "tsc apps/stimulator/check-unused-selectors.ts && node apps/stimulator/check-unused-selectors",
    "merge-coverage": "tsc merge-coverage.ts && node merge-coverage",
    "release": "ng build --prod --base-href=/ --output-hashing none",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stechy1/diplomka-frontend"
  },
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "~9.1.9",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@nrwl/angular": "9.3.0",
    "@stechy1/diplomka-share": "^1.0.23",
    "angular-2-local-storage": "^3.0.2",
    "bootstrap": "^4.5.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "core-js": "^3.6.5",
    "fuse.js": "^6.0.2",
    "intro.js": "^2.9.3",
    "ng2-charts": "^2.3.2",
    "ng5-slider": "^1.2.4",
    "ngx-logger": "^4.1.9",
    "ngx-socket-io": "^3.0.1",
    "ngx-toastr": "^12.1.0",
    "npm-check-updates": "^6.0.1",
    "rxjs": "~6.5.5",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.7",
    "@angular/cli": "9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@nrwl/cypress": "9.3.0",
    "@nrwl/jest": "9.3.0",
    "@nrwl/workspace": "9.3.0",
    "@types/intro.js": "^2.4.6",
